digraph {
	node [shape=circle]
	S0
	node [shape=circle]
	S1
	node [shape=doublecircle]
	S2
	node [shape=circle]
	S3
	node [shape=circle]
	S4
	node [shape=none]
	""
	"" -> S0
	S0 -> S1 [label=a]
	S0 -> S1 [label=b]
	S0 -> S3 [label=a]
	S0 -> S3 [label=c]
	S0 -> S4 [label=b]
	S0 -> S4 [label=c]
	S1 -> S1 [label=a]
	S1 -> S1 [label=b]
	S1 -> S2 [label=c]
	S3 -> S3 [label=a]
	S3 -> S3 [label=c]
	S3 -> S2 [label=b]
	S4 -> S2 [label=a]
	S4 -> S4 [label=b]
	S4 -> S4 [label=c]
}
